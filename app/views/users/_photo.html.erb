<div class = "col-sm-3">

  <div id = "photo">
    <%= link_to image_tag(photo.photo.url(:medium)), user_photo_path(:user_id => params[:id], :id => photo.id), id: 'photo-img' %>

    <div id = "photo-overlay">
      &nbsp;Uploaded <%= photo.created_at.strftime("%m/%d/%y") %>
    </div>
  </div>
</div>